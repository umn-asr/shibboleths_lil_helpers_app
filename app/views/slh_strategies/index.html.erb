<h1>Listing slh_strategies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>SP Entity ID</th>
    <th>Actions</th>
  </tr>

<% @slh_strategies.each do |slh_strategy| %>
  <tr>

    <td><%= slh_strategy.name %></td>
    <td><%= slh_strategy.sp_entity_id %></td>
    <td>
      <%= link_to "Add host", new_user_slh_config_dir_slh_strategy_slh_host_path(@user,@slh_config_dir,slh_strategy) %>
      <%= link_to "Show", user_slh_config_dir_slh_strategy_path(@user,@slh_config_dir,slh_strategy) %>
      <%= link_to "Edit", edit_user_slh_config_dir_slh_strategy_path(@user,@slh_config_dir,slh_strategy) %>
   
    </td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Slh strategy', new_user_slh_config_dir_slh_strategy_path(@user,@slh_config_dir)%>
